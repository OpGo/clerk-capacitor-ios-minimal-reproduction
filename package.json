{
  "name": "clerk-capacitor-minimal-reproduction",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build-app-copy": "npm run build && npx cap copy",
    "build-app-sync": "npm run build && npx cap sync",
    "generate-assets": "capacitor-assets generate --iconBackgroundColor '#000000' --iconBackgroundColorDark '#000000' --splashBackgroundColor '#000000' --splashBackgroundColorDark '#000000' --assetPath ./src/assets",
    "ionic:serve": "vite dev --host"
  },
  "dependencies": {
    "@aparajita/tailwind-ionic": "^2.2.0",
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "6.0.1",
    "@capacitor/core": "6.2.0",
    "@capacitor/device": "^6.0.1",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/push-notifications": "^6.0.2",
    "@capacitor/status-bar": "6.0.1",
    "@clerk/clerk-js": "^5.39.0",
    "@clerk/clerk-react": "^5.16.0",
    "@clerk/elements": "^0.19.8",
    "@ionic/react": "^8.2.7",
    "@ionic/react-router": "^8.2.7",
    "@ionic/storage": "^4.0.0",
    "capacitor-secure-storage-plugin": "^0.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "6.1.2",
    "@eslint/js": "^9.9.0",
    "@nabla/vite-plugin-eslint": "^2.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "sass": "^1.77.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  }
